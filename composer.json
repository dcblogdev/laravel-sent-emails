{
    "name": "jmztaylor/laravel-sent-emails",
    "description": "Store outgoing emails in Laravel",
    "keywords": [
        "jmztaylor",
        "laravel",
        "sent-emails"
    ],
    "homepage": "https://github.com/jmztaylor/laravel-sent-emails",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "David Carr",
            "email": "dave@dcblog.dev",
            "role": "Developer"
        }
    ],
    "require": {
